    "vuex": "4.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/preset-env": "7.13.0",
    "@vue/compiler-sfc": "3.0.5",
    "babel-core": "6.26.3",
    "babel-loader": "8.2.2",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "7.0.3",
    "css-loader": "5.0.2",
    "cssnano": "4.1.10",
    "html-webpack-plugin": "5.2.0",
