    "report": "npm run clean && cross-env NODE_ENV=production webpack --progress --color --config ./config/webpack.report.config.js --mode production",
    "eslint": "eslint --fix --ext \"src/**/*.{ts.vue,less,css,scss}\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
